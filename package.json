{
  "name": "tyranid-tdgen",
  "version": "0.2.2",
  "description": "Generate type definition files from tyranid schemas",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "bin": {
    "tyranid-tdgen": "bin/tyranid-tdgen"
  },
  "scripts": {
    "build": "rm -rf ./dist/ && npm run tsc",
    "pretest": "npm run build",
    "prepublish": "npm run test",
    "ava": "mkdir -p generated && nyc ava",
    "test": "npm run ava && npm run compile-test",
    "compile-test": "tsc -p ./test/compile-test/ && tsc -p ./test/compile-client/",
    "tsc": "tsc -d --pretty",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "docs": "typedoc --options typedoc.json src",
    "gh-pages": "npm run docs && gh-pages -d docs",
    "prettier": "prettier --single-quote --write ./{src,test}/**/*.ts ./{src,test,example}/*.ts",
    "precommit": "npm run prettier && npm test"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "author": "bsouthga@gmail.com",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/mongodb": "2.2.7",
    "ava": "0.16.0",
    "codecov": "1.0.1",
    "gh-pages": "1.0.0",
    "husky": "0.14.3",
    "mongodb": "2.2.30",
    "nyc": "8.3.0",
    "prettier": "1.5.2",
    "ts-node": "^3.2.0",
    "typedoc": "0.7.1",
    "typescript": "2.4.1",
    "tyranid": "0.2.0"
  },
  "peerDependencies": {
    "tyranid": "^0.2.0"
  },
  "ava": {
    "files": [
      "dist/test/index.js"
    ]
  },
  "nyc": {
    "include": [
      "dist/src"
    ]
  },
  "dependencies": {
    "@types/commander": "2.9.1",
    "@types/lodash": "4.14.65",
    "@types/node": "6.0.52",
    "commander": "2.9.0",
    "lodash": "4.17.2"
  }
}
