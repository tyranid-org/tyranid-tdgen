{
  "name": "tyranid-tdgen",
  "version": "0.0.1",
  "description": "Generate type definition files from tyranid schemas",
  "main": "dist-es5/index.js",
  "scripts": {
    "babel": "./node_modules/.bin/babel ./dist-es6 --out-dir ./dist-es5",
    "build": "rm -rf ./dist-es6/ ./dist-es5/ && npm run typings && npm run lint && npm run tsc && npm run babel",
    "copy-typings": "cp -r ./typings ./dist-es6 && cp -r ./typings ./dist-es5",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/ava",
    "tsc": "./node_modules/typescript/bin/tsc -d --pretty",
    "typings": "./node_modules/.bin/typings install",
    "lint": "./node_modules/tslint/bin/tslint -c ./tslint.json ./{test,lib}/**/*.ts && echo '> \\033[0;32mlinter passed!\\033[0m'"
  },
  "author": "bsouthga@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.16.0",
    "nyc": "^7.1.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "tyranid": "^0.1.4",
    "tslint": "^3.5.0",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015-node4": "2.0.3"
  },
  "peerDependencies": {
    "tyranid": "^0.1.4"
  },
  "babel": {
    "presets": [
      "es2015-node4"
    ],
    "sourceMaps": "inline"
  },
  "ava": {
    "files": [
      "dist-es5/test/index.js"
    ]
  },
  "nyc": {
    "include": [
      "dist-es5/src"
    ]
  },
  "dependencies": {
    "lodash": "^4.15.0"
  }
}
