{
  "name": "tyranid-tdgen",
  "version": "0.0.27",
  "description": "Generate type definition files from tyranid schemas",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "bin": {
    "tyranid-tdgen": "bin/tyranid-tdgen"
  },
  "scripts": {
    "build": "rm -rf ./dist/ && npm run tsc",
    "pretest": "npm run build",
    "prepublish": "npm run test",
    "ava": "./node_modules/.bin/nyc ./node_modules/.bin/ava",
    "test": "npm run ava && npm run compile-test",
    "compile-test": "mkdir -p ./generated && ./node_modules/.bin/tsc -p ./test/compile-test/ && ./node_modules/.bin/tsc -p ./test/compile-client/",
    "tsc": "./node_modules/typescript/bin/tsc -d --pretty",
    "coverage": "./node_modules/.bin/nyc report --reporter=text-lcov > coverage.lcov && ./node_modules/.bin/codecov",
    "docs": "typedoc --options typedoc.json src",
    "gh-pages": "npm run docs && gh-pages -d docs",
    "prettier": "./node_modules/.bin/prettier --single-quote --write ./src/**/*.ts ./test/**/*.ts",
    "precommit": "npm run prettier && npm test"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "author": "bsouthga@gmail.com",
  "license": "Apache-2.0",
  "devDependencies": {
    "ava": "0.16.0",
    "codecov": "1.0.1",
    "gh-pages": "^0.12.0",
    "husky": "^0.13.4",
    "nyc": "8.3.0",
    "prettier": "^1.4.4",
    "typedoc": "^0.7.1",
    "typescript": "2.3.4",
    "tyranid": "0.1.57"
  },
  "peerDependencies": {
    "tyranid": "^0.1.57"
  },
  "ava": {
    "files": [
      "dist/test/index.js"
    ]
  },
  "nyc": {
    "include": [
      "dist/src"
    ]
  },
  "dependencies": {
    "@types/commander": "2.9.1",
    "@types/lodash": "4.14.65",
    "@types/node": "6.0.52",
    "commander": "2.9.0",
    "lodash": "4.17.2"
  }
}
